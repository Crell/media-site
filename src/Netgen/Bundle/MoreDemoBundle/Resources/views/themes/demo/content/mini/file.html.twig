{% set location_path = path(location) %}

<article class="view-type view-type-{{ viewType }} file vl5">
    <header class="article-header with-icon">
        <a href="{{ location_path }}"><i class="fas fa-file-o"></i></a>
        <h2 class="title"><a href="{{ location_path }}">{{ ng_render_field(content.fields.name) }}</a></h2>
    </header>
</article>
